<body>
    <div id="bg">
        <!-- <div><img src="images/combined.png" alt="上面的條" width="100%"></div> -->
        <nav class="navbar navbar-expand-sm bg-light">
            <ul class="navbar-nav">
              <li id="line">｜</li>
              <li class="nav-item">
                <a class="nav-link" href="index">食魂</a>
              </li>
