                    <a class="nav-link" href="gameform.html">註冊</a>
                <li id="line">｜</li>
                  </li>
                </ul>
            </ul>
            </nav>

        <div class="container border">
            <form method="post" action="/character_info" encType="multipart/form-data">
                <div id="logo" class="container mr-5 pr-5 border">
                    <a href="tsai_shi" style="text-align: right;"><img src="images/稀有度按鈕.png"  width="15%"></a>
                </div>
                <!-- SSR -->
                <div class="container border">
                    <div id="SSR" class="container ml-3 pr-5 border">
                        <img src="images/bar_SSR.png" alt="SSR_bar" width="100%">
                    </div>
                    <div class="container ml-5 pb-5 border">
                        <div class="row">
                            <% character.forEach(function(item){if(item.rarity == "SSR"){%>
                            <div class="col-sm-6 col-md-4 col-lg-2 mr-5 mb-3 border">
                                <img src="images/head/<%= item.name %>.png" alt="" title="" />
                                <button type="submit"  name="name" id="<%= item.name %>" value="<%= item.name %>" style="width: 100%;background-color: #4caf4f00;background-image: url(images/按鈕_new.png);background-repeat: no-repeat;
                                    background-size: cover;
                                    border:none;
                                    margin-left:5px;
                                    " class="btn btn-primary"><%= item.name %></button>
                            </div>
                            <%} }) %>
                        </div>
                    </div>
                </div>
                <!-- SR -->
                <div class="container border">
                    <div id="SR" class="container ml-3 pr-5 border">
                        <img src="images/bar_SR.png" alt="SR_bar" width="100%">
                    </div>
                    <div class="container ml-5 pb-5 border">
                        <div class="row">
                            <% character.forEach(function(item){if(item.rarity == "SR"){ %>
                                <div class="col-sm-6 col-md-4 col-lg-2 mr-5 mb-3 border">
                                    <img src="images/head/<%= item.name %>.png" alt="" title="" />
                                    <button type="submit"  name="name" id="<%= item.name %>" value="<%= item.name %>" style="width: 100%;background-color: #4caf4f00;background-image: url(images/按鈕_new.png);background-repeat: no-repeat;
                                        background-size: cover;
                                        border:none;
                                        margin-left:5px;
                                        " class="btn btn-primary"><%= item.name %></button>
                                </div>
                            <%} }) %>

                        </div>
                    </div>
                </div>
                <!-- R -->
                <div class="container border">
                    <div id="R" class="container ml-3 pr-5 border">
                        <img src="images/bar_R.png" alt="R_bar" width="100%">
                    </div>
                    <div class="container ml-5 pb-5 border">
                        <div class="row">
                            <% character.forEach(function(item){if(item.rarity == "R"){ %>
                                <div class="col-sm-6 col-md-4 col-lg-2 mr-5 mb-3 border">
                                    <img src="images/head/<%= item.name %>.png" alt="" title="" />
                                    <button type="submit"  name="name" id="<%= item.name %>" value="<%= item.name %>" style="width: 100%;background-color: #4caf4f00;background-image: url(images/按鈕_new.png);background-repeat: no-repeat;
                                        background-size: cover;
                                        border:none;
                                        margin-left:5px;
                                        " class="btn btn-primary"><%= item.name %></button>
                                </div>
                            <%} }) %>
                        </div>

                    </div>
                </div>
                <!-- N -->
                <div class="container border">
                    <div id="N" class="container ml-3 pr-5 border">
                        <img src="images/bar_N.png" alt="N_bar" width="100%">
                    </div>
                    <div class="container ml-5 pb-5 border">
                        <div class="row">
                            <% character.forEach(function(item){if(item.rarity == "N"){ %>
                                <div class="col-sm-6 col-md-4 col-lg-2 mr-5 mb-3 border">
                                    <img src="images/head/<%= item.name %>.png" alt="" title="" />
                                    <button type="submit"  name="name" id="<%= item.name %>" value="<%= item.name %>" style="width: 100%;background-color: #4caf4f00;background-image: url(images/按鈕_new.png);background-repeat: no-repeat;
                                        background-size: cover;
                                        border:none;
                                        margin-left:5px;
                                        " class="btn btn-primary"><%= item.name %></button>
                                </div>
                            <%} }) %>
                        </div>

                    </div>
                </div>
            </form>
        </div>
        

    </div>

</body>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.slim.min.js"></script>
